APP_ENV=production
NODE_ENV=production

# Origins
FRONTEND_ORIGIN=https://app.example.com
PUBLIC_API_BASE_URL=https://api.example.com
PUBLIC_FRONTEND_URL=https://app.example.com
PUBLIC_WEBHOOK_BASE=https://api.example.com/api/integrations
STORE_PUBLIC_ORIGIN=https://store.example.com
PUBLIC_WEB_BASE_URL=https://example.com
CORS_ORIGINS=https://app.example.com,https://staging.example.com

# Mongo (ReplicaSet)
MONGODB_URI=mongodb://admin:${MONGO_PASS}@mongo:27017/musaidbot?replicaSet=rs0&authSource=admin
DATABASE_URL=${MONGODB_URI}

# Redis / Rabbit / Qdrant
REDIS_URL=redis://redis:6379
RABBIT_URL=amqp://kaleem:${RABBIT_PASS}@rabbitmq:5672/kleem
QDRANT_URL=http://qdrant:6333

# MinIO
MINIO_ENDPOINT=minio
MINIO_PORT=9000
MINIO_USE_SSL=false
MINIO_REGION=us-east-1
MINIO_BUCKET=documents
MINIO_PUBLIC_URL=https://cdn.example.com
MINIO_ACCESS_KEY=__SET_IN_SECRETS__
MINIO_SECRET_KEY=__SET_IN_SECRETS__

# n8n
N8N_BASE_URL=https://n8n.example.com
N8N_API_URL=${N8N_BASE_URL}
N8N_API_KEY=__SET_IN_SECRETS__
N8N_INCOMING_PATH=/webhook/ai-agent-{merchantId}
N8N_SIGNING_SECRET=__SET_IN_SECRETS__

# Mail
MAIL_HOST=smtp.example.com
MAIL_PORT=465
MAIL_SECURE=true
MAIL_USER=support@example.com
MAIL_PASS=__SET_IN_SECRETS__
MAIL_FROM=support@example.com

# JWT / AI keys (secrets)
JWT_SECRET=__SET_IN_SECRETS__
OPENAI_API_KEY=__SET_IN_SECRETS__
GEMINI_API_KEY=__SET_IN_SECRETS__

# Feature/config vars
RABBIT_CONFIRM_TIMEOUT_MS=10000
CHAT_N8N_ENDPOINT=/webhook/webhooks/kleem/incoming
CHAT_BOT_NAME=kleem
CHAT_DEFAULT_CHANNEL=webchat
CHAT_TYPING_STOP_DELAY_MS=2000
EMBEDDINGS_EXPECTED_DIM=384
EMBEDDINGS_HTTP_TIMEOUT_MS=15000
EMBEDDINGS_RX_TIMEOUT_MS=20000
EMBEDDINGS_MAX_TEXT_LENGTH=10000
EMBEDDINGS_MAX_RETRIES=3
EMBEDDINGS_BASE_RETRY_DELAY_MS=500
EMBEDDINGS_ENDPOINT_PATH=/embed
SEC_HSTS_MAX_AGE=31536000
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=500
RATE_LIMIT_CODE=RATE_LIMIT_EXCEEDED
RATE_LIMIT_TEXT=تم تجاوز حد الطلبات، الرجاء المحاولة لاحقاً
