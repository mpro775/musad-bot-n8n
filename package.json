  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start:prod": "node dist/main",
    "build": "nest build",
    "postbuild": "copyfiles -u 1 \"src/i18n/**/*\" dist/src",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "backfill:merchants": "ts-node -r tsconfig-paths/register src/scripts/backfill-merchants.ts",
    "start:worker": "ts-node -r tsconfig-paths/register src/main.worker.ts",
    "prestart:dev": "copyfiles -u 1 \"src/i18n/**/*\" dist/src",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "generate-specs": "ts-node generate-jest-specs.ts",
    "test": "jest --runInBand",
    "test:unit": "jest --runInBand --selectProjects unit",
    "test:integration": "jest --runInBand --selectProjects integration",
    "test:e2e": "jest --runInBand --selectProjects e2e",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:coverage": "jest --coverage --selectProjects unit",
    "test:cov": "npm run test:coverage",
    "migrate:msg-ids": "npx ts-node src/scripts/backfill-message-ids.ts",
    "seed:admin": "ts-node -r tsconfig-paths/register src/scripts/seed-admin.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:security": "powershell -ExecutionPolicy Bypass -File test/e2e/run-security-tests.ps1",
    "test:security:bash": "bash test/e2e/run-security-tests.sh",
    "test:webhooks": "jest test/e2e/webhooks --verbose",
    "test:auth": "jest test/e2e/auth --verbose",
    "test:watch:cov": "jest --coverage --watchAll --selectProjects unit",
    "coverage:serve": "live-server coverage/lcov-report --port=6060 --no-browser",
    "dev:testcov": "concurrently -k \"npm:coverage:serve\" \"npm:test:watch:cov\"",
    "prepare": "husky",
    "lint": "eslint . --ext .ts,.tsx,.js --cache --cache-location .eslintcache",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix --cache --cache-location .eslintcache",
    "lint:strict": "eslint . --ext .ts,.tsx,.js --max-warnings=0 --cache --cache-location .eslintcache",
    "report:init": "rimraf reports && node -e \"require('fs').mkdirSync('reports',{recursive:true})\"",
    "report:eslint": "eslint . --ext .ts,.tsx,.js -f unix -o reports/eslint-unix.txt -f json -o reports/eslint.json",
    "report:prettier": "prettier --check \"src/**/*.{ts,js}\" \"test/**/*.ts\" > reports/prettier.txt",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "report:typecheck": "npm run typecheck > reports/typescript.txt",
    "report:jest": "jest --runInBand --coverage --coverageReporters=text-summary,lcov > reports/jest.txt",
    "ci:report": "npm run report:init && npm run report:eslint && npm run report:prettier && npm run report:typecheck && npm run report:jest",
    "audit:ci": "npm audit --omit=dev --audit-level=high",
    "depcheck": "depcheck"
  },
